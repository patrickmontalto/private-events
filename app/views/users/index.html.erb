<div class="row">
	<div class="col-md-8 col-md-offset-2">
		<div class="panel panel-info">
			<div class="panel-heading">All Users</div>
			<div class="panel-body">
				<% @users.each do |user| %>
					<li><%= link_to "#{user.full_name} - #{user.email}", user %></li> 
				<% end %>
			</div>
		</div>
	</div>
</div>

